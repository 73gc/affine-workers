{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"jsx": "react",
		"module": "es2022",
		"moduleResolution": "node",
		"types": ["@cloudflare/workers-types"],
		"resolveJsonModule": true,
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"verbatimModuleSyntax": true,
		"esModuleInterop": true,
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true
	}
}
